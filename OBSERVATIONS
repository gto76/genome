set style data linespoints; plot 'tmp1' title "Skorpijon", 'tmp2' title "Kacica", 'tmp3' title "Clovek", 'tmp4' title "Kobilica", 'tmp5' title "Ligenj", 'tmp6' title "Stonoga", 'tmp7' title "Zaba", 'tmp9' title "Riba", 'tmp10' title "Meduza", 'tmp11' title "Morska Zvezda"
